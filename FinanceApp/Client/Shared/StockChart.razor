<SfStockChart @ref="Chart">
    <StockChartSeriesCollection>
        <StockChartSeries DataSource="Finances" Type="ChartSeriesType.Candle" XName="T" YName="C" High="H" Low="L" Open="O" Close="C" Volume="V" />
    </StockChartSeriesCollection>
</SfStockChart>

@code {

    [Parameter]
    public List<StockFinanceDTO> Finances { get; set; }
    public SfStockChart Chart { get; set; }

}